{% extends '_layout' %}

{% block main %}
<h1> {{ entry.title }}</h1>
<h2> {{ entry.blogIntroduction }}</h2>
{{ entry.blogBody }}
<p>Posted by <a href='#'>{{ entry.author}}</a> on {{ entry.postDate | date('M d, Y') }}</p>

{% for category in entry.category %}

<a href ='{{ category.url }} '>{{ category }}</a>

{% endfor %}
{% endblock %}
